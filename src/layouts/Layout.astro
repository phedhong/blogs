---

import SlotLayout from "./_slots/SlotLayout.astro"

import BaseHead from "./_elements/BaseHead.astro"
import Header from '@components/preact/Header'
import Footer from '@components/preact/Footer'
import Breadcrumbs from '@components/astro/Breadcrumbs.astro'
import {Alert}  from '@components/svelte/alert'

const d = true;


interface Props {
	title: string;
}

const { title } = Astro.props.frontmatter || Astro.props;

---


  
  <SlotLayout> 

 <BaseHead {title} slot="common-head" /> 


    <Header slot="page-header" />

    <Breadcrumbs />

    <slot width="w-[100%]" />

    <Footer slot="page-footer" />
    <Alert message={Object.keys(Astro)} client:visible />

  </SlotLayout>
